{
  "name": "@quid/react-core",
  "version": "2.4.0",
  "description": "Core React Quid components",
  "main": "dist/index.js",
  "main:umd": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quid/refraction/tree/master/packages/react-core"
  },
