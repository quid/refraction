  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/css": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "@emotion/styled-base": "^10.0.4",
    "@quid/theme": "^2.3.0",
    "color": "^3.1.0"
  },
  "peerDependencies": {
    "react": "15||16",
    "react-router-dom": "^4.0.0||^5.0.0"
  }
