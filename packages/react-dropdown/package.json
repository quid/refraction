    "@emotion/styled-base": "^10.0.4",
    "@quid/react-core": "^3.3.0",
    "@quid/theme": "^3.3.0",
    "color": "^3.1.0",
    "downshift": "^3.2.0",
    "popper.js": "^1.14.7",
    "react-popper": "^1.3.3"
  }
