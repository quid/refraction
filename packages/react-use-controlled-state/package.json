    "test": "cd ../.. && yarn test --testPathPattern packages/react-use-controlled-state"
  },
  "devDependencies": {
    "flow-copy-source": "^2.0.2",
    "microbundle": "^0.8.3",
    "react": "^16.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
