    "@emotion/styled-base": "^10.0.4",
    "@quid/react-core": "^4.0.0",
    "@quid/theme": "^4.0.0",
    "date-fns": "^1.30.1",
    "emotion": "^10.0.6"
  },
  "peerDependencies": {
    "react": "15||16"
  }
}
