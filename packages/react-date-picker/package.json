{
  "name": "@quid/react-date-picker",
  "version": "4.0.0",
  "description": "Lightweight date picker component",
  "main": "dist/index.js",
  "main:umd": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quid/refraction/tree/master/packages/react-date-picker"
  },
  "publishConfig": {
